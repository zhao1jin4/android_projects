<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.itcast.phone"
    android:versionCode="1"
    android:versionName="Beta">

    <uses-sdk android:minSdkVersion="16" android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <!-- //查看源码时<activity android:permission="android.permission.CALL_PHONE" -->
    <application android:icon="@drawable/congsmall" android:label="@string/app_name" android:allowBackup="true">
        <activity android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>



</manifest>